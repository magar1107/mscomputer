{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/web/$1"
    }
  ],
  "buildCommand": "flutter build web --release",
  "devCommand": "flutter run --web-server-bind 0.0.0.0",
  "installCommand": "flutter pub get",
  "outputDirectory": "build/web"
}
